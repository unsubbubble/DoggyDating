extends layout.jade

block nav
	header
		nav
			div.navbar.row
				div.tall-col.col-3
					a(href="discover.html")
						img(src="images/logo.png", alt="DoggyDates Logo")
				div.tall-col.col-5
				div.tall-col.col-1
					a(href="discover.html")
						span.fa.fa-search.fa-big
				div.tall-col.col-1
					a(href="messages.html")
						span.fa.fa-comment.fa-big
				div.tall-col.col-1
					div.dropdown
						button.dropdown-btn(onclick="myFunction()")
							span.fa.fa-user.fa-big
							span.fa.fa-caret-down.fa-small
						div.dropdown-content(id="myDropdown")
							a(href="profile.html") Profile
							a(href="index.html") Logout
					a(href="profile.html")
					
	script(src="js/dropdown.js")
					
	block content