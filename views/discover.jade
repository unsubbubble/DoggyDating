extends auth_base.jade

block content
	if(user)
		h1 user
	div.dog-discover-profile
		div.row
			div.tall-col.col-4
			div.tall-col.col-4
				h2.center-text Find playmates in your area!
				br
				div.discover-container
					div.img-container
						img(id="id_discover_image", src="images/dogs/1.jpg", alt="Dog Profile Picture")
					div.row
						div.tall-col.col-1
						div.tall-col.col-2
							a(href="#", onclick="discover();")
								span.fa.fa-times.fa-extra-big
						div.tall-col.col-2
							a(href="#", onclick="discover();")
								span.fa.fa-heart.fa-extra-big.fa-red
			div.tall-col.col-4
			
	script(src="js/discover.js")